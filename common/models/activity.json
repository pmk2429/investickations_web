    "name": {
      "type": "string",
      "required": true
    },
    "num_of_people": {
      "type": "number"
    },
    "num_of_pets": {
      "type": "string"
    },
    "num_of_ticks": {
      "type": "number"
    },
    "location_area": {
      "type": "string"
    },
    "timestamp": {
      "type": "date",
      "required": true
    },
    "created_at": {
      "type": "date",
      "required": true
    },
    "updated_at": {
      "type": "string",
